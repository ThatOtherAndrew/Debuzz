{
  "manifest_version": 3,
  "name": "Debuzz",
  "version": "1.0.0",
  "descriprion": "the ultimate tool to debuzz any web page !",
  "permissions": ["activeTab", "scripting"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": "assets/icon.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
